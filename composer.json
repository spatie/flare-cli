{
    "name": "spatie/flare-cli",
    "description": "A standalone CLI tool for Flare error tracking and performance monitoring.",
    "keywords": ["flare", "spatie", "error-tracking", "cli", "monitoring"],
    "homepage": "https://flareapp.io",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/spatie/flare-cli/issues",
        "source": "https://github.com/spatie/flare-cli"
    },
    "authors": [
        {
            "name": "Spatie",
            "email": "info@spatie.be",
            "homepage": "https://spatie.be"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/http": "^12.17",
        "laravel-zero/framework": "^12.0.2",
        "spatie/laravel-openapi-cli": "*@dev"
    },
    "require-dev": {
        "laravel/pint": "^1.25.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.4|^4.1.2"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../laravel-openapi-cli",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["flare"]
}
